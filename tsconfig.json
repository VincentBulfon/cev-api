{
  "compilerOptions": {
    "sourceMap": true,
    "module": "commonjs",
    "target": "ES2018",
    "outDir": "./dist",
    "lib": ["ESNext", "DOM"],
    // "strict": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "strictBindCallApply": true,
    "strictFunctionTypes": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "noUnusedParameters": false
  }
}
